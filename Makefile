# Minimal Zerynth Makefile

compile :
	ztc project compile .

uplink :
	ztc project uplink .

deps :
	ztc project deps .

clean :
	ztc project clean .

register :
	ztc project register .

virtualize :
	ztc project virtualize .

ports :
	ztc project ports .

targets :
	ztc project targets .

vms :
	ztc project vms .

prepare :
	ztc project prepare .

console : 
	ztc project console .
